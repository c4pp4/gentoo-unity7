## human-icon-theme.tar.xz downloaded from:
## https://github.com/LibreOffice/core/tree/3498ed37527b7af795f00dc6a55c11e65a313186/icon-themes/human

ebuild_hook() {
	ehook_use libreoffice_theme \
		&& unpack "${EHOOK_FILESDIR}"/human-icon-theme.tar.xz \
		&& unpack "${EHOOK_FILESDIR}"/tango-icon-theme.tar.xz
}
