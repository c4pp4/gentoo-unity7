change() {
	ebegin "Running cpufreq-set ${*}"
	/usr/libexec/cpufrequtils-change.sh "${@}" > /dev/null
	eend $?
}

start() {
	change ${START_OPTS} -- ${SYSFS_EXTRA}
}

stop() {
	change "${STOP_OPTS}"
}
